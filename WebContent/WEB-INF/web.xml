<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Java_beca_coritel_loginapp</display-name>
	<servlet>
		<servlet-name>loginServlet</servlet-name>
		<servlet-class>com.sergiorb.loginapp.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logoutServlet</servlet-name>
		<servlet-class>com.sergiorb.loginapp.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>dashboardServlet</servlet-name>
		<servlet-class>com.sergiorb.loginapp.servlets.DashboardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>dashboardServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
</web-app>